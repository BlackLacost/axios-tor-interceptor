{
  "name": "axios-tor-interceptor",
  "version": "0.0.5",
  "description": "Axios Tor Interceptor",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --watch",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "jest && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "typedoc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackLacost/axios-tor-interceptor.git"
  },
  "keywords": [
    "axios",
    "tor",
    "interceptor"
  ],
  "author": "Ilya Lisin <blacklacost@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BlackLacost/axios-tor-interceptor/issues"
  },
  "homepage": "https://github.com/BlackLacost/axios-tor-interceptor#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "socks-proxy-agent": "^6.1.1"
  }
}
